<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/customlist_relativelayout"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/package_list_selector"
    android:orientation="horizontal"
    android:paddingLeft="5dip" >

    <!--  ListRow Left sied Thumbnail image -->
	<LinearLayout 
	    android:id="@+id/myduty_city_linear_pickup_point" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"		
        android:layout_alignParentLeft="true" 
	    android:layout_marginRight="5dip"
	    android:orientation="horizontal">
        
	    <!-- Title Of Song-->
	    <TextView
	        android:id="@+id/myduty_city_txt_pickup_point"
	        android:layout_width="125dp"
	        android:layout_height="wrap_content"
	        android:text="Pickup Point"
	        android:textColor="#040404"
	        android:typeface="sans" 
	        android:textSize="15dip"
	        android:textStyle="bold"/>
	    
	    <!-- Artist Name -->
	    <TextView
	        android:id="@+id/myduty_city_txt_pickup_point_details"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:textColor="#343434"
	        android:textSize="15dip"
	        android:layout_marginTop="1dip"
	        android:text="" />
        
	</LinearLayout>
	
	<!--  ListRow Left sied Thumbnail image -->
	<LinearLayout 
	    android:id="@+id/myduty_city_linear_time" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_below="@id/myduty_city_linear_pickup_point"		
        android:layout_alignParentLeft="true" 
	    android:layout_marginRight="5dip"
	    android:orientation="horizontal">
        
	    <!-- Title Of Song-->
	    <TextView
	        android:id="@+id/myduty_city_txt_time"
	        android:layout_width="125dp"
	        android:layout_height="wrap_content"
	        android:text="Time"
	        android:textColor="#040404"
	        android:typeface="sans" 
	        android:textSize="15dip"
	        android:textStyle="bold"/>
	    
	    <!-- Artist Name -->
	    <TextView
	        android:id="@+id/myduty_city_txt_time_details"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:textColor="#343434"
	        android:textSize="15dip"
	        android:layout_marginTop="1dip"
        android:text="" />
        
	</LinearLayout>
	
	
	<!--  ListRow Left sied Thumbnail image -->
	<LinearLayout 
	    android:id="@+id/myduty_city_linear_customername" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"	
		android:layout_below="@id/myduty_city_linear_time"			
        android:layout_alignParentLeft="true" 
	    android:layout_marginRight="5dip"
	    android:orientation="horizontal">
        
	    <!-- Title Of Song-->
	    <TextView
	        android:id="@+id/myduty_city_txt_customername"
	        android:layout_width="125dp"
	        android:layout_height="wrap_content"
	        android:text="Customer Name"
	        android:textColor="#040404"
	        android:typeface="sans" 
	        android:textSize="15dip"
	        android:textStyle="bold"/>
	    
	    <!-- Artist Name -->
	    <TextView
	        android:id="@+id/myduty_city_txt_customername_details"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:textColor="#343434"
	        android:textSize="15dip"
	        android:layout_marginTop="1dip"
	        android:text="" />
        
	</LinearLayout>
	
	<!--  ListRow Left sied Thumbnail image -->
	<LinearLayout 
	    android:id="@+id/myduty_city_linear_mobilenumber" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"	
		android:layout_below="@id/myduty_city_linear_customername"		
        android:layout_alignParentLeft="true" 
	    android:layout_marginRight="5dip"
	    android:orientation="horizontal"
	    android:visibility="gone">
        
	    <!-- Title Of Song-->
    	<TextView
	        android:id="@+id/myduty_city_txt_mobilenumber"
	        android:layout_width="125dp"
	        android:layout_height="wrap_content"
	        android:text="Mobile Number"
	        android:textColor="#040404"
	        android:typeface="sans" 
	        android:textSize="15dip"
	        android:textStyle="bold"/>
	    
	    <!-- Artist Name -->
	    <TextView
	        android:id="@+id/myduty_city_txt_mobilenumber_details"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:textColor="#343434"
	        android:textSize="15dip"
	        android:layout_marginTop="1dip"
	        android:text="" />
        
	</LinearLayout>
	
	<!--  ListRow Left sied Thumbnail image -->
	<LinearLayout 
	    android:id="@+id/myduty_city_linear_drop_point" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"	
		android:layout_below="@id/myduty_city_linear_mobilenumber"
        android:layout_alignParentLeft="true" 
	    android:layout_marginRight="5dip"
	    android:orientation="horizontal">
        
	    <!-- Title Of Song-->
	    <TextView
	        android:id="@+id/myduty_city_txt_drop_point"
	        android:layout_width="125dp"
	        android:layout_height="wrap_content"
	        android:text="Drop Point"
	        android:textColor="#040404"
	        android:typeface="sans" 
	        android:textSize="15dip"
	        android:textStyle="bold"/>
	    
	    <!-- Artist Name -->
	    <TextView
	        android:id="@+id/myduty_city_txt_drop_point_details"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:textColor="#343434"
	        android:textSize="15dip"
	        android:layout_marginTop="1dip"
	        android:text="" />
        
	</LinearLayout>

	
      
     <!-- Rightend Arrow -->    
     <ImageView 
        android:id="@+id/pendinglist_arrow"
        android:layout_width="wrap_content"
     	android:layout_height="wrap_content"
     	android:src="@drawable/arrow"
     	android:layout_alignParentRight="true"
     	android:layout_centerVertical="true"/>
     
     <!-- Rightend Duration -->

   <!--  <TextView
        android:id="@+id/pl_item_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/pl_item_linear_to"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="60dip"
        android:gravity="right"
        android:text="5:45"
        android:textColor="#040404"
        android:textSize="14dip"
        android:textStyle="bold" /> -->

</RelativeLayout>
